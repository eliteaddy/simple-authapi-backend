{
  "version": 2,
  "builds": [
    { "src": "index.js", "use": "@now/node-server" },
    { "src": "admin.js", "use": "@now/node-server" }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/server.js",
      "methods": ["GET"]
    },
    {
      "src": "/",
      "dest": "/server.js",
      "methods": ["POST"]
    },
    {
      "src": "/([^/]+)",
      "dest": "/?id=$1",
      "methods": ["DELETE"]
    },
    {
      "src": "/([^/]+)",
      "dest": "/?id=$1",
      "methods": ["PATCH"]
    },
    {
      "src": "/admin",
      "dest": "/admin.js",
      "methods": ["GET"]
    },
    {
      "src": "/admin",
      "dest": "/admin.js",
      "methods": ["POST"]
    },
    {
      "src": "/admin/([^/]+)",
      "dest": "/admin/?id=$1",
      "methods": ["DELETE"]
    },
    {
      "src": "/admin/([^/]+)",
      "dest": "/admin/?id=$1",
      "methods": ["PATCH"]
    }
  ]
}
